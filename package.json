    },
    "devDependencies": {
        "@fullhuman/postcss-purgecss": "^2.1.0",
        "@fullhuman/vue-cli-plugin-purgecss": "~2.2.0",
        "@mdi/font": "^5.4.55",
        "@types/debounce": "^1.2.0",
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
